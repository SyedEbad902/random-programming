let myTuple: [string, number] = ["typescript", 2024];

console.log(myTuple[0]); 
console.log(myTuple[1]);


myTuple[0] = "js";
myTuple[1] = 2025;

console.log(myTuple);

let myTuple: [string, number] = ["typescript", 2024];
let [language, year] = myTuple;

console.log(language); // Output: "typescript"
console.log(year); // Output: 2024

